  <h:body>
    <select1 ref="/hh_listing_2/enumerator_team">
      <label>Welcome to the sample household listing form, meant mostly to provide some sample data for exploration and export. 

ENUMERATOR: Which enumeration team are you on?</label>
      <item>
        <label>Northern Team</label>
        <value>northern</value>
      </item>
      <item>
        <label>Southern Team</label>
        <value>southern</value>
      </item>
      <item>
        <label>Eastern Team</label>
        <value>eastern</value>
      </item>
      <item>
        <label>Western Team</label>
        <value>western</value>
      </item>
    </select1>
    <select1 ref="/hh_listing_2/enumerator">
      <label>ENUMERATOR: Which enumerator are you?</label>
      <itemset nodeset="instance('enumerator')/root/item[filter= /hh_listing_2/enumerator_team ]">
        <value ref="name"/>
        <label ref="jr:itext(itextId)"/>
      </itemset>
    </select1>
    <select1 ref="/hh_listing_2/province">
      <label>ENUMERATOR: What province are you in?</label>
      <item>
        <label>Manicaland</label>
        <value>Manicaland</value>
      </item>
      <item>
        <label>Mashonaland Central</label>
        <value>Mashonaland_Central</value>
      </item>
    </select1>
    <select1 ref="/hh_listing_2/district">
      <label>ENUMERATOR: What district are you in?</label>
      <itemset nodeset="instance('district')/root/item[filter= /hh_listing_2/province ]">
        <value ref="name"/>
        <label ref="jr:itext(itextId)"/>
      </itemset>
    </select1>
    <input ref="/hh_listing_2/gps_location">
      <label>ENUMERATOR: Please record your location.</label>
      <hint>GPS coordinates can only be collected when outside.</hint>
    </input>
    <select1 ref="/hh_listing_2/consent">
      <label>ENUMERATOR: Does an adult in this household give informed consent?</label>
      <item>
        <label>Yes</label>
        <value>1</value>
      </item>
      <item>
        <label>No</label>
        <value>0</value>
      </item>
    </select1>
    <group ref="/hh_listing_2/on_consent">
      <label>Questions if have consent</label>
      <input ref="/hh_listing_2/on_consent/respondent_name">
        <label>What is your name?</label>
      </input>
      <input ref="/hh_listing_2/on_consent/address">
        <label>What is your address?</label>
      </input>
      <input ref="/hh_listing_2/on_consent/num_in_hh">
        <label>How many household members do you have?</label>
      </input>
      <upload mediatype="image/*" ref="/hh_listing_2/on_consent/household_photo">
        <label>ENUMERATOR: Please photograph the outside of the household.</label>
      </upload>
    </group>
  </h:body>
