      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/fip_phase_1/starttime" publishable="false()" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/fip_phase_1/endtime" publishable="false()" type="dateTime"/>
      <bind jr:preload="property" jr:preloadParams="deviceid" nodeset="/fip_phase_1/deviceid" publishable="false()" type="string"/>
      <bind jr:preload="property" jr:preloadParams="subscriberid" nodeset="/fip_phase_1/subscriberid" publishable="false()" type="string"/>
      <bind jr:preload="property" jr:preloadParams="simserial" nodeset="/fip_phase_1/simid" publishable="false()" type="string"/>
      <bind jr:preload="property" jr:preloadParams="phonenumber" nodeset="/fip_phase_1/devicephonenum" publishable="false()" type="string"/>
      <bind jr:preload="property" jr:preloadParams="username" nodeset="/fip_phase_1/username" publishable="false()" type="string"/>
      <bind calculate="duration()" nodeset="/fip_phase_1/duration" publishable="false()" type="string"/>
      <bind jr:preload="property" jr:preloadParams="caseid" nodeset="/fip_phase_1/caseid" publishable="false()" type="string"/>
      <bind nodeset="/fip_phase_1/intronote" original="note" readonly="true()" required="false()" type="string"/>
      <bind nodeset="/fip_phase_1/geographic_levels/country_level" type="string"/>
      <bind nodeset="/fip_phase_1/geographic_levels/country" required="true()" type="select1"/>
      <bind nodeset="/fip_phase_1/geographic_levels/district_level" type="string"/>
      <bind nodeset="/fip_phase_1/geographic_levels/district_coop_id" required="true()" type="select1"/>
      <bind nodeset="/fip_phase_1/geographic_levels/community_level" type="string"/>
      <bind nodeset="/fip_phase_1/geographic_levels/district_coop_desc" type="string"/>
      <bind nodeset="/fip_phase_1/geographic_levels/community_desc" type="string"/>
      <bind nodeset="/fip_phase_1/geographic_levels/community_id" required="true()" type="select1"/>
      <bind nodeset="/fip_phase_1/geographic_levels/society_level" type="string"/>
      <bind nodeset="/fip_phase_1/geographic_levels/society_desc" type="string"/>
      <bind nodeset="/fip_phase_1/geographic_levels/society_id" required="true()" type="select1"/>
      <bind nodeset="/fip_phase_1/farmer_id" required="true()" type="select1"/>
      <bind nodeset="/fip_phase_1/farmer_name" type="string"/>
      <bind calculate="pulldata('farmer_fips', 'recoms', 'ID_key',  /fip_phase_1/farmer_id )" nodeset="/fip_phase_1/farmer_recoms" type="string"/>
      <bind calculate="substr( /fip_phase_1/farmer_recoms ,1, string-length( /fip_phase_1/farmer_recoms )-1)" nodeset="/fip_phase_1/farmer_recom_ids_list" type="string"/>
      <bind calculate="count-items(',',  /fip_phase_1/farmer_recom_ids_list )" nodeset="/fip_phase_1/num_recoms" type="string"/>
      <bind nodeset="/fip_phase_1/farmer_recoms_note" original="note" readonly="true()" required="false()" type="string"/>
      <bind calculate=" /fip_phase_1/num_recoms " nodeset="/fip_phase_1/farmer_fip_count" readonly="true()" type="string"/>
      <bind calculate="item-at(',',  /fip_phase_1/farmer_recom_ids_list , index()-1)" nodeset="/fip_phase_1/farmer_fip/recom_id" type="string"/>
      <bind calculate="if( /fip_phase_1/geographic_levels/country =1, 'recommendation_fr', 'recommendation')" nodeset="/fip_phase_1/farmer_fip/recom_column" type="string"/>
      <bind calculate="pulldata('fip_recommendations',  /fip_phase_1/farmer_fip/recom_column , 'rid', int( /fip_phase_1/farmer_fip/recom_id ))" nodeset="/fip_phase_1/farmer_fip/relevant_recom" publishable="false()" type="string"/>
      <bind nodeset="/fip_phase_1/farmer_fip/fip_recoms/act_recom" original="note" readonly="true()" required="false()" type="string"/>
      <bind nodeset="/fip_phase_1/farmer_fip/fip_recoms/est_budget" required="true()" type="decimal"/>
      <bind constraint="date(format-date-time(., '%Y-%m-01')) &gt;= date(format-date-time(today(), '%Y-%m-01'))" jr:constraintMsg="The start month cannot be earlier than the current month" nodeset="/fip_phase_1/farmer_fip/fip_recoms/start_month" required="true()" type="date"/>
      <bind constraint="date(.) &gt;= date( /fip_phase_1/farmer_fip/fip_recoms/start_month )" jr:constraintMsg="The end month cannot be earlier than the start month" nodeset="/fip_phase_1/farmer_fip/fip_recoms/end_month" required="true()" type="date"/>
      <bind nodeset="/fip_phase_1/farm_size_spec/farm_size" required="true()" type="decimal"/>
      <bind constraint=". &gt;= 1 and . &lt;=  /fip_phase_1/farm_size_spec/farm_size " jr:constraintMsg="jr:itext('/fip_phase_1/farm_size_spec/farm_size_agreed:jr:constraintMsg')" nodeset="/fip_phase_1/farm_size_spec/farm_size_agreed" publishable="true()" required="true()" type="decimal"/>
      <bind calculate=" /fip_phase_1/num_recoms " nodeset="/fip_phase_1/fip_summary_count" readonly="true()" type="string"/>
      <bind calculate="indexed-repeat( /fip_phase_1/farmer_fip/relevant_recom ,  /fip_phase_1/farmer_fip , index())" nodeset="/fip_phase_1/fip_summary/recom_row" type="string"/>
      <bind calculate="format-number(indexed-repeat( /fip_phase_1/farmer_fip/fip_recoms/est_budget ,  /fip_phase_1/farmer_fip , index()))" nodeset="/fip_phase_1/fip_summary/budget_row" type="string"/>
      <bind calculate="indexed-repeat( /fip_phase_1/farmer_fip/fip_recoms/est_budget ,  /fip_phase_1/farmer_fip , index()) *  /fip_phase_1/farm_size_spec/farm_size_agreed " nodeset="/fip_phase_1/fip_summary/tot_budget_row_unformatted" type="string"/>
      <bind calculate="format-number(indexed-repeat( /fip_phase_1/farmer_fip/fip_recoms/est_budget ,  /fip_phase_1/farmer_fip , index()) *  /fip_phase_1/farm_size_spec/farm_size_agreed )" nodeset="/fip_phase_1/fip_summary/tot_budget_row" type="string"/>
      <bind calculate="format-date-time(indexed-repeat( /fip_phase_1/farmer_fip/fip_recoms/start_month ,  /fip_phase_1/farmer_fip , index()), '%b-%Y')" nodeset="/fip_phase_1/fip_summary/start_month_row" type="string"/>
      <bind calculate="format-date-time(indexed-repeat( /fip_phase_1/farmer_fip/fip_recoms/end_month ,  /fip_phase_1/farmer_fip , index()), '%b-%Y')" nodeset="/fip_phase_1/fip_summary/end_month_row" type="string"/>
      <bind calculate="index()" nodeset="/fip_phase_1/fip_summary/recom_count" type="string"/>
      <bind nodeset="/fip_phase_1/fip_summary/tot_est_budget_header" original="note" readonly="true()" required="false()" type="string"/>
      <bind nodeset="/fip_phase_1/fip_summary/summary_row_note" original="note" readonly="true()" required="false()" type="string"/>
      <bind nodeset="/fip_phase_1/fip_summary/committed" publishable="true()" required="true()" type="select1"/>
      <bind calculate="format-number(sum-if( /fip_phase_1/fip_summary/tot_budget_row_unformatted ,  /fip_phase_1/fip_summary/committed  = 1))" nodeset="/fip_phase_1/tot_est_budget_from_recoms" type="string"/>
      <bind nodeset="/fip_phase_1/total_est_budget" original="note" readonly="true()" required="false()" type="string"/>
      <bind nodeset="/fip_phase_1/farmer_signature" required="false()" type="binary"/>
      <bind nodeset="/fip_phase_1/farmer_photo" relevant=" /fip_phase_1/farmer_signature  = ''" required="true()" type="binary"/>
      <bind jr:requiredMsg="The signature of the field staff conducting the data collection is required." nodeset="/fip_phase_1/enumerator_signature" required="true()" type="binary"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/fip_phase_1/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <input ref="/fip_phase_1/intronote">
      <label ref="jr:itext('/fip_phase_1/intronote:label')"/>
    </input>
    <group ref="/fip_phase_1/geographic_levels">
      <label ref="jr:itext('/fip_phase_1/geographic_levels:label')"/>
      <input appearance="invisible" calculate="1" ref="/fip_phase_1/geographic_levels/country_level"/>
      <select1 appearance="search('full_geographic_level_dataset', 'matches', 'level',  /fip_phase_1/geographic_levels/country_level )" ref="/fip_phase_1/geographic_levels/country">
        <label ref="jr:itext('/fip_phase_1/geographic_levels/country:label')"/>
        <item>
          <label>level_name</label>
          <value>country_id</value>
        </item>
      </select1>
      <input appearance="invisible" calculate="2" ref="/fip_phase_1/geographic_levels/district_level"/>
      <select1 appearance="search('full_geographic_level_dataset', 'matches', 'level',  /fip_phase_1/geographic_levels/district_level , 'country_id',  /fip_phase_1/geographic_levels/country )" ref="/fip_phase_1/geographic_levels/district_coop_id">
        <label ref="jr:itext('/fip_phase_1/geographic_levels/district_coop_id:label')"/>
        <item>
          <label>level_name</label>
          <value>district_coop_id</value>
        </item>
      </select1>
      <input appearance="invisible" calculate="3" ref="/fip_phase_1/geographic_levels/community_level"/>
      <input appearance="invisible" calculate="concat( /fip_phase_1/geographic_levels/country_level , '_',  /fip_phase_1/geographic_levels/country , '_', /fip_phase_1/geographic_levels/district_coop_id )" ref="/fip_phase_1/geographic_levels/district_coop_desc"/>
      <input appearance="invisible" calculate="concat( /fip_phase_1/geographic_levels/country_level , '_',  /fip_phase_1/geographic_levels/country , '_',  /fip_phase_1/geographic_levels/community_level )" ref="/fip_phase_1/geographic_levels/community_desc"/>
      <select1 appearance="search('full_geographic_level_dataset', 'matches','level',  /fip_phase_1/geographic_levels/community_level , 'district_coop_desc',  /fip_phase_1/geographic_levels/district_coop_desc )" ref="/fip_phase_1/geographic_levels/community_id">
        <label ref="jr:itext('/fip_phase_1/geographic_levels/community_id:label')"/>
        <item>
          <label>level_name</label>
          <value>community_id</value>
        </item>
      </select1>
      <input appearance="invisible" calculate="4" ref="/fip_phase_1/geographic_levels/society_level"/>
      <input appearance="invisible" calculate="concat( /fip_phase_1/geographic_levels/country_level , '_',  /fip_phase_1/geographic_levels/country , '_',  /fip_phase_1/geographic_levels/district_coop_id , '_',  /fip_phase_1/geographic_levels/community_id )" ref="/fip_phase_1/geographic_levels/society_desc"/>
      <select1 appearance="search('full_geographic_level_dataset', 'matches', 'level',  /fip_phase_1/geographic_levels/society_level , 'society_desc',  /fip_phase_1/geographic_levels/society_desc )" ref="/fip_phase_1/geographic_levels/society_id">
        <label ref="jr:itext('/fip_phase_1/geographic_levels/society_id:label')"/>
        <item>
          <label>level_name</label>
          <value>level_id</value>
        </item>
      </select1>
    </group>
    <select1 appearance="search('farmers', 'matches', 'colcen_id',  /fip_phase_1/geographic_levels/society_id )" ref="/fip_phase_1/farmer_id">
      <label ref="jr:itext('/fip_phase_1/farmer_id:label')"/>
      <item>
        <label>name</label>
        <value>ID_key</value>
      </item>
    </select1>
    <input appearance="invisible" calculate="pulldata('farmers', 'full_name', 'ID_key', /fip_phase_1/farmer_id  )" ref="/fip_phase_1/farmer_name"/>
    <input appearance="0" ref="/fip_phase_1/farmer_recoms_note">
      <label ref="jr:itext('/fip_phase_1/farmer_recoms_note:label')"/>
      <hint ref="jr:itext('/fip_phase_1/farmer_recoms_note:hint')"/>
    </input>
    <group ref="/fip_phase_1/farmer_fip">
      <label ref="jr:itext('/fip_phase_1/farmer_fip:label')"/>
      <repeat appearance="table" jr:count=" /fip_phase_1/farmer_fip_count " nodeset="/fip_phase_1/farmer_fip">
        <group appearance="field-list" ref="/fip_phase_1/farmer_fip/fip_recoms">
          <label ref="jr:itext('/fip_phase_1/farmer_fip/fip_recoms:label')"/>
          <input ref="/fip_phase_1/farmer_fip/fip_recoms/act_recom">
            <label ref="jr:itext('/fip_phase_1/farmer_fip/fip_recoms/act_recom:label')"/>
            <hint ref="jr:itext('/fip_phase_1/farmer_fip/fip_recoms/act_recom:hint')"/>
          </input>
          <input appearance="show_formatted" ref="/fip_phase_1/farmer_fip/fip_recoms/est_budget">
            <label ref="jr:itext('/fip_phase_1/farmer_fip/fip_recoms/est_budget:label')"/>
          </input>
          <input appearance="month-year" ref="/fip_phase_1/farmer_fip/fip_recoms/start_month">
            <label ref="jr:itext('/fip_phase_1/farmer_fip/fip_recoms/start_month:label')"/>
          </input>
          <input appearance="month-year" ref="/fip_phase_1/farmer_fip/fip_recoms/end_month">
            <label ref="jr:itext('/fip_phase_1/farmer_fip/fip_recoms/end_month:label')"/>
          </input>
        </group>
      </repeat>
    </group>
    <group appearance="field-list" ref="/fip_phase_1/farm_size_spec">
      <label ref="jr:itext('/fip_phase_1/farm_size_spec:label')"/>
      <input ref="/fip_phase_1/farm_size_spec/farm_size">
        <label ref="jr:itext('/fip_phase_1/farm_size_spec/farm_size:label')"/>
      </input>
      <input ref="/fip_phase_1/farm_size_spec/farm_size_agreed">
        <label ref="jr:itext('/fip_phase_1/farm_size_spec/farm_size_agreed:label')"/>
      </input>
    </group>
    <group ref="/fip_phase_1/fip_summary">
      <label ref="jr:itext('/fip_phase_1/fip_summary:label')"/>
      <repeat appearance="field-list" jr:count=" /fip_phase_1/fip_summary_count " nodeset="/fip_phase_1/fip_summary">
        <input ref="/fip_phase_1/fip_summary/tot_est_budget_header">
          <label ref="jr:itext('/fip_phase_1/fip_summary/tot_est_budget_header:label')"/>
          <hint ref="jr:itext('/fip_phase_1/fip_summary/tot_est_budget_header:hint')"/>
        </input>
        <input ref="/fip_phase_1/fip_summary/summary_row_note">
          <label ref="jr:itext('/fip_phase_1/fip_summary/summary_row_note:label')"/>
        </input>
        <select1 ref="/fip_phase_1/fip_summary/committed">
          <label ref="jr:itext('/fip_phase_1/fip_summary/committed:label')"/>
          <item>
            <label ref="jr:itext('/fip_phase_1/fip_summary/committed/1:label')"/>
            <value>1</value>
          </item>
          <item>
            <label ref="jr:itext('/fip_phase_1/fip_summary/committed/0:label')"/>
            <value>0</value>
          </item>
        </select1>
      </repeat>
    </group>
    <input ref="/fip_phase_1/total_est_budget">
      <label ref="jr:itext('/fip_phase_1/total_est_budget:label')"/>
      <hint ref="jr:itext('/fip_phase_1/total_est_budget:hint')"/>
    </input>
    <upload appearance="signature" mediatype="image/*" ref="/fip_phase_1/farmer_signature">
      <label ref="jr:itext('/fip_phase_1/farmer_signature:label')"/>
      <hint ref="jr:itext('/fip_phase_1/farmer_signature:hint')"/>
    </upload>
    <upload mediatype="image/*" ref="/fip_phase_1/farmer_photo">
      <label ref="jr:itext('/fip_phase_1/farmer_photo:label')"/>
      <hint ref="jr:itext('/fip_phase_1/farmer_photo:hint')"/>
    </upload>
    <upload appearance="signature" mediatype="image/*" ref="/fip_phase_1/enumerator_signature">
      <label ref="jr:itext('/fip_phase_1/enumerator_signature:label')"/>
      <hint ref="jr:itext('/fip_phase_1/enumerator_signature:hint')"/>
    </upload>
  </h:body>
</h:html>
